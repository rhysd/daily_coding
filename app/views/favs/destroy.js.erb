
var answer_id = "<%= j(render 'answer_id') %>";
if ($('#answer-' + answer_id + ' a.fav-btn > img').attr('src') == '/assets/star_full.png') {
  $('#answer-' + answer_id + ' a.fav-btn > img').attr('src','/assets/star_empty.png');
}

$('#answer-' + answer_id + ' a.fav-btn').attr('href', "/favs/create/" + answer_id);
